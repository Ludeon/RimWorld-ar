#: Defs/ThingDefs_Buildings/Buildings_Production.xml:6
msgctxt "CraftingSpot.label"
msgid "crafting spot"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:7
msgctxt "CraftingSpot.description"
msgid ""
"A place for crafting simple items like knives or bows. Production here is "
"slow because of the lack of tools."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:65
msgctxt "ButcherSpot.label"
msgid "butcher spot"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:66
msgctxt "ButcherSpot.description"
msgid ""
"A place for butchering animals. Due to the lack of equipment, butchering "
"here yields only 70% of the meat and leather of each creature."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:153
msgctxt "TableSculpting.label"
msgid "art bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:154
msgctxt "TableSculpting.description"
msgid "A workbench equipped for creating art."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:219
msgctxt "TableButcher.label"
msgid "butcher table"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:220
msgctxt "TableButcher.description"
msgid ""
"A heavy table for butchering dead creatures into pieces of raw meat. Rotting"
" or dessicated corpses cannot be butchered."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:287
msgctxt "HandTailoringBench.label"
msgid "hand tailor bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:288
msgctxt "HandTailoringBench.description"
msgid ""
"A workbench for tailoring clothes by hand. Works at 50% of the speed of an "
"electric tailoring bench."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:351
msgctxt "ElectricTailoringBench.label"
msgid "electric tailor bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:352
msgctxt "ElectricTailoringBench.description"
msgid ""
"A workbench with a sewing machine for rapid tailoring of clothes. Can work "
"without electricity at 50% of normal speed."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:428
msgctxt "FueledSmithy.label"
msgid "fueled smithy"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:429
msgctxt "FueledSmithy.description"
msgid "A wood-fueled station for smithing non-mechanical weapons and tools."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:504
msgctxt "ElectricSmithy.label"
msgid "electric smithy"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:505
msgctxt "ElectricSmithy.description"
msgid ""
"An electric-powered station for smithing non-mechanical weapons and tools."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:580
msgctxt "TableMachining.label"
msgid "machining table"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:581
msgctxt "TableMachining.description"
msgid ""
"A work station for assembling machinery like guns and ammunition, or "
"breaking down dead mechanoids."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:658
msgctxt "ElectricStove.label"
msgid "electric stove"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:659
msgctxt "ElectricStove.description"
msgid ""
"An electrically-powered stove with an attached countertop for preparing "
"meals."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:750
msgctxt "FueledStove.label"
msgid "fueled stove"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:751
msgctxt "FueledStove.description"
msgid "A wood-fueled stove with an attached countertop for preparing meals."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:841
msgctxt "TableStonecutter.label"
msgid "stonecutter's table"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:842
msgctxt "TableStonecutter.description"
msgid ""
"A work table with saws and chisels for cutting stone chunks into usable "
"blocks."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:912
msgctxt "Brewery.label"
msgid "brewery"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:913
msgctxt "Brewery.description"
msgid ""
"A work station with all the equipment needed to mix wort for beer "
"production. Wort must be fermented to finally become beer."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:969
msgctxt "DrugLab.label"
msgid "drug lab"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:970
msgctxt "DrugLab.description"
msgid ""
"A work bench equipped with containers, heaters, and measurement devices for "
"producing various drugs."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1036
msgctxt "ElectricSmelter.label"
msgid "electric smelter"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1037
msgctxt "ElectricSmelter.description"
msgid ""
"Extracts usable metal from slag chunks and other mixed metal items. Consumes"
" a lot of power."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1118
msgctxt "BiofuelRefinery.label"
msgid "biofuel refinery"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1119
msgctxt "BiofuelRefinery.description"
msgid "Refines biological matter like wood or plant matter into chemfuel."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1187
msgctxt "FabricationBench.label"
msgid "fabrication bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1188
msgctxt "FabricationBench.description"
msgid ""
"A workbench equipped with advanced tools for producing technological marvels"
" from simpler materials."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1266
msgctxt "SimpleResearchBench.label"
msgid "simple research bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1267
msgctxt "SimpleResearchBench.description"
msgid ""
"A simple bench with writing implements and simple measurement devices. "
"Researchers work here to discover new things."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1325
msgctxt "HiTechResearchBench.label"
msgid "hi-tech research bench"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1326
msgctxt "HiTechResearchBench.description"
msgid ""
"A high-tech bench with computers and electronic measurement equipment. "
"Allows more rapid research, and unlocks advanced research projects."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1403
msgctxt "ElectricCrematorium.label"
msgid "electric crematorium"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1404
msgctxt "ElectricCrematorium.description"
msgid ""
"A huge stone crematorium which vaporizes corpses with extremely high "
"temperatures."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1490
msgctxt "HydroponicsBasin.label"
msgid "hydroponics basin"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1491
msgctxt "HydroponicsBasin.description"
msgid ""
"An artificial nutrient bath for growing plants. The nutrients pumps must "
"work continuously; the plants will die if power is cut."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1550
msgctxt "FermentingBarrel.label"
msgid "fermenting barrel"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1551
msgctxt "FermentingBarrel.description"
msgid "A barrel for fermenting raw wort into beer."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1607
msgctxt "DeepDrill.label"
msgid "deep drill"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1608
msgctxt "DeepDrill.description"
msgid ""
"A deep-crust drilling rig for extracting minerals from deep under the "
"surface. Use a ground-penetrating scanner to detect the resources. This "
"drill will yield stone chunks if no resources are present. Can be "
"repositioned freely."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1680
msgctxt "NutrientPasteDispenser.label"
msgid "nutrient paste dispenser"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1681
msgctxt "NutrientPasteDispenser.description"
msgid ""
"A machine that synthesizes edible nutrient paste from organic feedstocks "
"placed in adjacent hoppers. It consumes less ingredients and time than any "
"other meal production method - but nobody likes eating nutrient paste. "
"Accepts raw food, but not rough plant matter like hay."
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1752
msgctxt "Hopper.label"
msgid "hopper"
msgstr ""

#: Defs/ThingDefs_Buildings/Buildings_Production.xml:1753
msgctxt "Hopper.description"
msgid "Holds resources for use by machines like nutrient paste dispensers."
msgstr ""
